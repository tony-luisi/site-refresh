{
  "name": "site-refresh",
  "version": "1.0.0",
  "description": "its my personal website",
  "main": "index.js",
  "scripts": {
    "start": "budo . -d --serve bundle.js --live",
    "build": "browserify . -o bundle.js -g uglifyify",
    "test": "browserify test.js | node | tap-spec",
    "dist": "mkdir -p dist && bash -c 'cp *.{js,html,css} dist'",
    "push": "gh-pages -d dist",
    "deploy": "npm run build && npm run dist && npm run push"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tony-luisi/site-refresh"
  },
  "author": "tony-luisi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tony-luisi/site-refresh/issues"
  },
  "homepage": "https://github.com/tony-luisi/site-refresh",
  "dependencies": {
    "gsap": "^1.18.4",
    "react": "^15.1.0",
    "react-bootstrap": "^0.29.4",
    "react-router-bootstrap": "^0.23.0",
    "react-typewrite": "^1.0.10",
    "react-typewriter": "^0.4.1",
    "react-typist": "^0.3.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "budo": "^8.2.0",
    "gh-pages": "^0.11.0",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-router": "^2.4.1",
    "tap-spec": "^4.1.1",
    "tape": "^4.5.1",
    "uglifyify": "^3.0.1",
    "watchify": "^3.7.0"
  }
}
